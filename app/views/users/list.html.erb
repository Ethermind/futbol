<div class="span-3">&nbsp;</div>
<div class="span-18">
	<fieldset>
		<legend>User's List, total users: <%= "#{@users.count}" %></legend>	
		<table border="0" cellspacing="0" cellpadding="0">
			<thead>
				<tr>
					<th class="span-4">Score</th>
					<th class="span-4">Email</th>
					<th class="span-4">Name</th>
					<th class="span-4 last">Password</th>
				</tr>
			</thead>
			<tbody>
				<% @users.each do |u| %>
				<tr>
					<td><%= u.score.round(1) %></td>
					<td><%= u.email %></td>
					<td><%= u.name %></td>
					<td><%= u.password %></td>
				</tr>
				<% end if @users %>
			</tbody>
		</table>
	</fieldset>
</div>
<div class="span-3 last">&nbsp;</div>