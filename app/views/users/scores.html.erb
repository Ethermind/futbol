<div class="span-3">&nbsp;</div>
<div class="span-18">
	<fieldset>
		<legend>User's Scores</legend>	
		<div id="users_scores_container" class="span-18" />
	</fieldset>
</div>
<div class="span-3 last">&nbsp;</div>
<script type="text/javascript"> 
$(function () {
    var chart;
    $(document).ready(function() {
        chart = new Highcharts.Chart({
            chart: {
                renderTo: 'users_scores_container',
                type: 'line'
            },
            title: {
                text: ""
            },
            yAxis: {
                title: {
                    text: 'Score'
                }
            },
            series: [
			<% @users.each_with_index do |user, index| %>
			<%= "," if index != 0 %>
				{
                	name: '<%= user.name %>',
                	data: <%= user.scores %>
            	}
			<% end %>
			]
        });
    });
});
</script>