<!DOCTYPE htm>
<html>
<head>
	<title>La cancha de tu hermana!</title>
	<%= stylesheet_link_tag "application", media: "all" %>
	<%= stylesheet_link_tag "screen", media: "screen, projection" %>
	<%= stylesheet_link_tag "print", media: "print" %>
	<%= csrf_meta_tags %>
  	<%= javascript_include_tag "application" %>
	<%= javascript_include_tag :defaults %>
	<script type="text/javascript">
	  var _gaq = _gaq || [];
	  _gaq.push(['_setAccount', 'UA-30668982-1']);
	  _gaq.push(['_trackPageview']);
	  (function() {
	    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	  })();
	</script>
</head>
<body>
	<div class="container">
		<div class="span-24 shade last">
			<div class="span-24 last">&nbsp;</div>
			<div class="span-2">&nbsp;</div>
			<div class="span-20">
				<%= render "/menu" %> 
			</div>
			<div class="span-2 last">&nbsp;</div>
			<div class="span-24 last">&nbsp;</div>
		</div>
		<% if flash[:error] %>
		<div class="span-24 red last">
			&nbsp;&nbsp;
			<strong><%= flash[:error] %></strong>
	    </div>
		<% end %>
		<div class="span-24 backwhite shadow last">&nbsp;</div>
		<div class="span-24 backwhite shadow last">
			<%= yield %>
		</div>
	</div>
</body>
</html>